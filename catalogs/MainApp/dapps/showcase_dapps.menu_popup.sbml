=begin popup

=begin actions
=object label: text="${title}", style=label_title
=object button: action=copy, media=text, text="${url}", post-action=popup-close, \
            label="URL 복사하기", \
            style=btn_menu
=object blank: style=blank_line

=object button: action=popup-close, label="취소", style=btn_close
=end actions

=end popup
