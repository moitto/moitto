import ~/themes.sbss

/cell: display=block, width=1pw, height=1ph, \
            border-color=$THEME_COLOR_01, border-width=1dp, page-background-color=$THEME_COLOR_00
/cell/widget: display=block, width=1ph, height=1ph, \
            position=absolute, gravity=left, background-color=$THEME_COLOR_01
/cell/title: display=block, width=0.3pw, height=20dp, \
            position=absolute, gravity=top-left, x="1ph+10dp", y=10dp, background-color=$THEME_COLOR_01
