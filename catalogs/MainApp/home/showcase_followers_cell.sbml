=begin cell

=object image: image-url="${userpic-url}", image-downloadable=yes, style=img_userpic
=object label: text="${username}", style=label_user


=object button: label="팔로우", script=follow, style=btn_follow
=object button: label="뮤트", script=mute, style=btn_mute


=begin form: form="form.${username}"
=object input: id="id", value="${id}"
=object input: id="username", value="${username}"
=end form

=object blank: style=blank_line

=end cell
