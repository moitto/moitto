=begin header

=object blank: style=blank_userpic.mask
=object button: image-url="${userpic-url}", image-downloadable=yes, style=btn_userpic
=object button: label="${author}", style=btn_user
=object label: text="${created-at}", type=date, format=ago, source-format="yyyy-MM-dd'T'HH:mm:ss", source-timezone=GMT, style=label_date

=begin body
${body}
=end body


=end header
