=begin cell

=object blank: style=blank_userpic.mask
=object image: image-url="${userpic-url}", reuse-id="${userpic-url}", style=img_userpic
=object label: text="${username}", style=label_user
=object label: text="00.00", style=label_price, display=none, alt="temporary"

=object label: text="${voted-at}", type=date, format=ago, source-format="yyyy-MM-dd'T'HH:mm:ss", source-timezone=GMT, style=label_date

=object blank: style=blank_line

=end cell
