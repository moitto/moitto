=begin cell

=object image: image-url="${image-url}", style=img_cover

=object section: section=contents, style=section_contents

=begin contents: id=contents

=begin title: alt="글 제목"
${title}
=end title


=begin meta: alt="태그, 보팅"
=(object button: script=show_tag, label="#${main-tag}", style=btn_meta, style=goB)==(object label: text=" • ", style=label_meta)==(object button: script=show_votes, type=decimal, label="${votes-count} ", style=btn_meta, style=goB)==(object button: script=show_votes, label="보팅", style=btn_meta)==(object label:text=" •", style=label_meta)==(object button: script=show_votes, label=" ${payout-value} ", style=btn_meta, style=goB)==(object label:text="• ", style=label_meta)==(object button: script=show_replies, label="댓글", style=btn_meta)==(object button: script=show_replies, label=" ${replies-count}", style=btn_meta, style=goB)=
=end meta


=object label: type=date, format=ago, source-format="yyyy-MM-dd'T'HH:mm:ss", source-timezone=GMT, text="${created-at}", style=label_date


=end contents

=end cell
